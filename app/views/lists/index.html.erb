<div class="container-up" class="center">
<img src="https://www.wiwo.de/images/die-kinos-eroeffnen/27375084/3-format1007.jpg" alt="cinema" class="center">
</div>

<div class="container d-flex justify-content-between">
  <p>My Lists</p>
  <button type="button" class="btn btn-success"><%= link_to 'create a movie list', new_list_path %></button>

</div>


<div>
  <ul>
  <% @lists.each do |list| %>
    <%=link_to list_path(list) do %>
      <li><%= list.name %></li>
    <% end %>
  <% end %>
  </ul>
</div>
