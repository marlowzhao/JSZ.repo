<div class="container-up">
  <p>Write about your movies</p>
</div>

<div class="container-mid">
</div>

<div class="container d-flex justify-content-between">
  <p>My Lists</p>

  <%= link_to 'create a movie list', new_list_path, class: "btn btn-success"%>

</div>

<div class="container">
  <ul>
  <% @lists.each do |list| %>
    <%=link_to list_path(list) do %>
      <li><%= list.name %></li>
    <% end %>
  <% end %>
  </ul>
</div>
